{
  "version": 2,
  "name": "mockstagram-gallery",
  "builds": [
    {
      "src": "package.json", 
      "use": "@vercel/static-build",
      "config": { "distDir": "." }
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|json|html|png|jpg|jpeg|gif|svg|ico))",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "framework": null,
  "outputDirectory": "./"
}